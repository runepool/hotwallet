{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/liquidium-dex/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/liquidium-dex/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/liquidium-dex",
  "projects": {
    "liquidium-dex": {
      "type": "application",
      "root": "apps/liquidium-dex",
      "entryFile": "main",
      "sourceRoot": "apps/liquidium-dex/src",
      "compilerOptions": {
        "tsConfigPath": "apps/liquidium-dex/tsconfig.app.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "wallet": {
      "type": "library",
      "root": "libs/wallet",
      "entryFile": "index",
      "sourceRoot": "libs/wallet/src",
      "compilerOptions": {
        "tsConfigPath": "libs/wallet/tsconfig.lib.json"
      }
    }
  }
}